    <script>
        $(document).ready(function () {
            $('input[name ="Rechercher"]').addClass("form-control").css("width", "200px");
        });
         $(document).ready(function () {
            $('#dataTables-example').dataTable();
            $('#dataTables-info3').dataTable();
            $('#dataTables-indus3').dataTable();
            $('#dataTables-gpmc3').dataTable();
            $('#dataTables-gtr3').dataTable();
            $('#dataTables-info4').dataTable();
            $('#dataTables-indus4').dataTable();
            $('#dataTables-gpmc4').dataTable();
            $('#dataTables-gtr4').dataTable();
        });
        var $combo = $(".classement");
        $combo.on("change", function () {
            var select = this,
                selected = $("option:selected", this).text();
            $combo.each(function (_, el) {
                if (el !== select) {
                    $("option", el).each(function (_, el) {
                        var $el = $(el);
                        if ($el.text() === selected) {
                            $el.remove();
                        }
                    });
                }
            });
        });
    </script>

