@model IEnumerable<ProjetConcoursAspCore.Models.File>

        <main class="main">
            <!-- Breadcrumb-->
            <ol class="breadcrumb">
                <li class="breadcrumb-item">Acceuil</li>
                <li class="breadcrumb-item">
                    <a href="/Admin/Etudiant3a/GetAllCondidatData">Anciens epreuves</a>
                </li>
                <li class="breadcrumb-item active">Liste des epreuves</li>
                <!-- Breadcrumb Menu-->
                <li class="breadcrumb-menu d-md-down-none">
                    <div class="btn-group" role="group" aria-label="Button group">
                        <a class="btn" href="#">
                            <i class="icon-speech"></i>
                        </a>
                        <a class="btn" href="./">
                            <i class="icon-graph"></i>  Acceuil
                        </a>
                        <a class="btn" href="#">
                            <i class="icon-settings"></i>  Paramètres
                        </a>
                    </div>
                </li>
            </ol>
            <div class="container-fluid">
                <div class="animated fadeIn">
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="card">
                                <div class="card-header">
                                    <i class="fa fade justify-content-center"></i> Table des anciens épreuves</b>
                                </div>

                                <div class="card-body">
                                    <div class="col-xs-12">
                                        <div class="panel panel-default">
                                            <div class="panel-heading">
                                                <div class="card-title">
                                                    <div class="title"></b></div>
                                                </div>
                                            </div>
                                            <div class="panel-body">
                                                <div class="table-responsive">



                                                    <table class="table table-striped table-bordered table-hover" id="dataTables-example">


                                                        <tr>

                                                            <th>
                                                                @Html.DisplayNameFor(model => model.Name)
                                                            </th>

                                                            <th>
                                                                @Html.DisplayNameFor(model => model.Annee)
                                                            </th>


                                                            <th>
                                                                Lien de Téléchargement
                                                            </th>

                                                            <th>
                                                                Action
                                                            </th>


                                                        </tr>


                                                        @foreach (var i in Model)
                                                        {
                                                    <tr>

                                                        <td>
                                                            @Html.DisplayFor(modelItem => i.Titre_concours)
                                                        </td>

                                                        <td>
                                                            @Html.DisplayFor(modelItem => i.Annee)
                                                        </td>

                                                        <td>

                                                            @Html.ActionLink(i.Name, "Get", "File", new { Id = i.Id }, new { title = "Open PDF document" })


                                                        </td>
                                                        <td>
                                                        @Html.ActionLink(" ", "delete", "File", new { Id = i.Id, Id_fil = ViewData["ID"] }, new { @class = "btn btn-danger fa fa-trash-o" })

                                                        </td>
                                                    </tr>
                                                        }

                                                    </table>



                                                </div>


                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </main>
